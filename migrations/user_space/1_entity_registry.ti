new_type("UserSpace");

.to_type("UserSpace");

set_enum("InstanceType", {
  SWITCH: "switch",
  LIGHT: "light",
  SENSOR: "sensor"
});

set_type("Instance", {
  unique_id: "#",
  type: "InstanceType?",
  instance_id: "str",
  created_at: "datetime",
  modified_at: "datetime",
  friendly_name: "str"
});

mod_type("UserSpace", "add", "InstanceRegistry", "{Instance}");
